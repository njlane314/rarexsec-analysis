{
    "plugins": [
      {
        "path": "build/CutFlowPlotPlugin.so",
        "analysis_configs": {
          "efficiency_plots": [
            {
              "region": "CUSTOM",
              "initial_label": "empty",
              "clauses": [
                "in_reco_fiducial",
                "num_slices == 1"
                "optical_filter_pe_beam > 20",
                "muon_score > 0.5",
                "muon_length > 10",
                "n_pfps_gen2 > 1"
              ]
            }
          ]
        },
        "plot_configs": {
          "efficiency_plots": [
            {
              "region": "CUSTOM",
              "channel_column": "inclusive_strange_channels",
              "signal_group": "inclusive_strange_channels",
              "log_y": true
            }
          ]
        }
      }
    ]
  }
